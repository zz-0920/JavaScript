{
  "name": "trae-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "client:dev": "vite",
    "build": "tsc -b && vite build",
    "build:client": "webpack --mode=development",
    "build:server": "tsc",
    "build:all": "npm run build:client && npm run build:server",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "server:dev": "nodemon",
    "server:start": "node api/server.js",
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "ssr:dev": "npm run build:client && npm run server:dev",
    "ssr:start": "npm run build:all && npm run server:start"
  },
  "dependencies": {
